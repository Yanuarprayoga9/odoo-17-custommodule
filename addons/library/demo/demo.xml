<odoo>
    <record id="demo_book_1" model="library.book">
        <field name="name">Book 1</field>
        <field name="author">Author 1</field>
        <field name="isbn">1234567890</field>
        <field name="available_copies">5</field>
    </record>

    <record id="demo_loan_1" model="library.loan">
        <field name="book_id" ref="demo_book_1"/>
        <field name="borrower_name">John Doe</field>
        <field name="loan_date">2024-12-19</field>
        <field name="return_date">2025-01-19</field>
        <field name="state">borrowed</field>
    </record>
</odoo>
